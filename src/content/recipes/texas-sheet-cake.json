{
  "title": "Texas Sheet Cake",
  "pubDate": "2025-02-04",
  "tags": [
    "cake",
    "dessert"
  ],
  "ingredients": {
    "sections": [
      {
        "title": "Cake",
        "ingredients": [
          "1 c butter",
          "1 c water",
          "2 c flour",
          "1 1/2 c sugar",
          "2 eggs",
          "1 1/2 c sour cream",
          "1 t almond extract",
          "1 t salt",
          "1 t baking soda"
        ]
      },
      {
        "title": "Frosting",
        "ingredients": [
          "1/2 c butter",
          "1/4 c milk",
          "4 c powdered sugar",
          "1/2 t almond extract"
        ]
      }
    ]
  },
  "directions": {
    "sections": [
      {
        "title": "Cake",
        "steps": [
          "Bring butter and water to a boil; remove from heat.",
          "Add rest of cake ingredients; stir until smooth.",
          "Pour into a greased cookie sheet.",
          "Bake at 325 for 15-17 mins, until cake is golden brown and passes the toothpick test.",
          "Cool for 20 mins."
        ]
      },
      {
        "title": "Frosting",
        "steps": [
          "Combine butter and milk; bring to a boil.",
          "Remove from heat. Add sugar and extract; mix well."
        ]
      }
    ]
  },
  "notes": [
    "Sondra's recipe."
  ]
}